<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

    <!--在这里继承了spring boot -->
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.3.0.RELEASE</version>
        <relativePath/>
    </parent>

    <modelVersion>4.0.0</modelVersion>


    <description>犀鸟商城</description>

    <groupId>com.toucan.shopping</groupId>
    <artifactId>toucan-shopping</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>



    <modules>
        <!-- =======================公共基础模块=========================-->

        <!--公共模块 -->
        <module>modules/common/toucan-shopping-common</module>
        <!--web公共模块 -->
        <module>modules/common/toucan-shopping-common-web</module>
        <!--公共模块 持久化 -->
        <module>modules/common/toucan-shopping-common-persistence</module>
        <!--权限注解 -->
        <module>modules/common/toucan-shopping-auth</module>
        <!-- 分布式锁-->
        <module>modules/common/toucan-shopping-lock-redis</module>
        <!-- 公共事件持久层 -->
        <module>modules/common/toucan-shopping-common-persistence-event</module>

        <!--用户中心操作es -->
        <module>modules/user/toucan-shopping-user-es</module>
        <!--用户中心实体VO -->
        <module>modules/user/toucan-shopping-user-entity-vo</module>

        <!--商品实体和VO -->
        <module>modules/product/toucan-shopping-product-entity-vo</module>
        <!--订单实体和VO -->
        <module>modules/order/toucan-shopping-order-entity-vo</module>
        <!-- 订单编号生成 -->
        <module>modules/order/toucan-shopping-order-no</module>
        <!--商品实体和VO -->
        <module>modules/category/toucan-shopping-category-entity-vo</module>
        <!--权限中心实体和VO -->
        <module>modules/adminAuth/toucan-shopping-admin-auth-entity-vo</module>
        <!--库存实体和VO -->
        <module>modules/stock/toucan-shopping-stock-entity-vo</module>
        <!--地区编码实体和VO -->
        <module>modules/area/toucan-shopping-area-entity-vo</module>

        <!-- ===============================================-->


        <!-- 微服务版 -->
        <module>toucan-shopping-cloud</module>


    </modules>


    <dependencies>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
            <optional>true</optional>
            <version>1.18.8</version>
        </dependency>

        <!--JSON解析 -->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>fastjson</artifactId>
            <version>1.2.56</version>
        </dependency>


        <!-- 对象属性拷贝 -->
        <dependency>
            <groupId>commons-beanutils</groupId>
            <artifactId>commons-beanutils</artifactId>
            <version>1.9.3</version>
        </dependency>


        <!--apache 公共包 -->
        <dependency>
            <groupId>org.apache.commons</groupId>
            <artifactId>commons-lang3</artifactId>
            <version>3.11</version>
        </dependency>

        <dependency>
            <groupId>org.slf4j</groupId>
            <artifactId>slf4j-api</artifactId>
        </dependency>



    </dependencies>

    <dependencyManagement>
        <dependencies>

            <!-- 我们的包-->
            <dependency>
                <groupId>com.toucan.shopping.modules.common</groupId>
                <artifactId>toucan-shopping-common</artifactId>
                <version>1.0-SNAPSHOT</version>
                <scope>compile</scope>
            </dependency>
            <dependency>
                <groupId>com.toucan.shopping.modules.common</groupId>
