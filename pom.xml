    <!--在这里继承了spring boot -->
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.3.0.RELEASE</version>
        <relativePath/>
    </parent>

    <modelVersion>4.0.0</modelVersion>


    <description>大嘴鸟商城</description>

    <groupId>com.toucan.shopping</groupId>
    <artifactId>toucan-shopping</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>pom</packaging>



    <modules>
        <!-- =======================公共基础模块=========================-->

        <!--公共模块 -->
        <module>modules/toucan-shopping-common</module>
        <!--web公共模块 -->
        <module>modules/toucan-shopping-common-web</module>
        <!--公共模块 持久化 -->
        <module>modules/toucan-shopping-common-persistence</module>
        <!--权限注解 -->
        <module>modules/toucan-shopping-auth</module>
        <!-- 分布式锁-->
        <module>modules/toucan-shopping-lock-redis</module>
        <!-- 订单编号生成 -->
        <module>modules/toucan-shopping-order-no</module>
        <!-- 公共事件持久层 -->
        <module>modules/toucan-shopping-common-persistence-event</module>

        <!--用户中心操作es -->
        <module>modules/toucan-shopping-user-es</module>

        <!-- ===============================================-->


        <!-- =======================基础服务层=========================-->
        <!--服务中心-->
        <module>base/toucan-shopping-eureka</module>
        <!-- 配置中心服务端-->
        <module>base/toucan-shopping-config-server</module>
        <!--网关服务 -->
        <module>base/toucan-shopping-gateway</module>
        <!-- ===============================================-->



        <!-- =======================业务服务层=========================-->
        <!--订单服务 -->
        <module>services/toucan-shopping-order</module>
        <!--商品服务 -->
        <module>services/toucan-shopping-product</module>
        <!--秒杀服务 -->
        <module>services/toucan-shopping-second-kill</module>
        <!--类别服务 -->
        <module>services/toucan-shopping-category</module>
        <!--库存服务 -->
        <module>services/toucan-shopping-stock</module>
        <!--地区编码服务 -->
        <module>services/toucan-shopping-area</module>
        <!-- 栏目服务 -->
        <module>services/toucan-shopping-column</module>



        <!--商品导出 -->
        <module>export/toucan-shopping-product-export</module>
        <!--订单导出 -->
        <module>export/toucan-shopping-order-export</module>
        <!--商品导出 -->
        <module>export/toucan-shopping-category-export</module>
        <!--用户中心导出 -->
        <module>export/toucan-shopping-user-export</module>
        <!--权限中心导出 -->
        <module>export/toucan-shopping-admin-auth-export</module>
        <!--库存导出 -->
        <module>export/toucan-shopping-stock-export</module>
        <!--地区编码导出 -->
        <module>export/toucan-shopping-area-export</module>


        <!--商品服务API -->
        <module>api/toucan-shopping-product-api</module>
        <!--订单服务API -->
        <module>api/toucan-shopping-order-api</module>
        <!--类别服务API -->
        <module>api/toucan-shopping-category-api</module>
        <!--用户中心API -->
        <module>api/toucan-shopping-user-api</module>
        <!--管理权限中心API -->
        <module>api/toucan-shopping-admin-auth-api</module>
        <!--库存API -->
        <module>api/toucan-shopping-stock-api</module>
        <!--地区编码API -->
        <module>api/toucan-shopping-area-api</module>



        <!--用户中心服务 -->
        <module>services/toucan-shopping-user</module>
        <!--管理员权限中心 -->
        <module>services/toucan-shopping-admin-auth</module>


        <!-- ===============================================-->


        <!-- =======================应用层=========================-->
        <!--处理前端请求 -->
        <module>apps/toucan-shopping-web</module>

        <!--后台管理端 -->
        <module>apps/toucan-shopping-admin</module>
        
        <!--用户中心界面 -->
        <module>apps/toucan-shopping-user-center-web</module>

        <!--权限中心界面 -->
        <module>apps/toucan-shopping-admin-auth-web</module>

        <!--定时任务调度-->
        <module>apps/toucan-shopping-scheduler</module>

        <!--用户中心任务调度 -->
        <module>apps/toucan-shopping-user-scheduler</module>
        <!-- ===============================================-->


        <!-- =====================插件层====================== -->
        <module>stater/toucan-shopping-admin-auth-spring-boot-starter</module>
        <!-- ===============================================-->





    </modules>


